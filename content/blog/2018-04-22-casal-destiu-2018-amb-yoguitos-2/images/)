            // Animación del logo del header:
            var logo_animation_maxSeconds = 20;
            var logo_animation_step = 0;

            jQuery(document).ready(function() {
//                start_logo_animation();
            });

            function start_logo_animation() {
                var interval = Math.floor((Math.random() * logo_animation_maxSeconds * 1000) + 1);
                window.setTimeout(logo_animation, interval);
            }

            function logo_animation() {
//                jQuery('.logo-animation').css('background-image','url(http://local-yoguitos/wp-content/themes/yoguitos/images/header-r-000.gif)');
//                jQuery('.logo-animation').css('background-image','url(wp-content/themes/yoguitos/images/header-r-000.gif)');
                logo_animation_step = 0;
                logo_animation_1();
            }

            // Piano animation
            function logo_animation_1() {
                switch (logo_animation_step++) {
                    case 0:
                        fileName = '-r-000';
                        break;
                    case 1:
                        fileName = '-r-100';
                        break;
                    case 2:
                        fileName = '-r-110';
                        break;
                    case 3:
                        fileName = '';
                        break;
                    default:
                        var interval = Math.floor((Math.random() * logo_animation_maxSeconds * 1000) + 1);
                        window.setTimeout(logo_animation, interval);
                        return;
                }
                jQuery('.logo-animation').css('background-image', 'url(/wp-content/themes/yoguitos/images/header' + fileName + '.gif)');
                window.setTimeout(logo_animation_1, 100);

            }
        </script>
